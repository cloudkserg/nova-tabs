{
    "name": "krisons/nova-tabs",
    "description": "Laravel Nova - Tabs.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Krisons\\Tabs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krisons\\Tabs\\TabsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
